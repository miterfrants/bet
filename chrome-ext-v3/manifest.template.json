{
  "name": "Homo.Bet",
  "version": "0.0.0",
  "description": "",
  "manifest_version": 2,
  "icons": {
    "16": "/imgs/16.png",
    "48": "/imgs/48.png",
    "120": "/imgs/120.png"
  },
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_popup": "popup/popup.html"
  },
  "permissions": ["declarativeContent", "storage", "https://*/*", "identity", "tabs"],
  "web_accessible_resources": [
    "fontawesome-free-5.9.0-web/css/all.css",
    "util/extended-prototype.js",
    "util/debounce.js",
    "components/board.js",
    "components/node.js",
    "page/dialog.html"
  ],
  "commands": {},
  "oauth2": {
    "client_id": "{clientId}",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  }
}